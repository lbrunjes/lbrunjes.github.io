<!doctype html>
<html>
<head>
<script src="../processing-1.3.6.js"></script>
<script type="text/processing" data-processing-target="graph">


int getY(x){
return eval(formulaY);
}


void setup(){

size(100,100);
frameRate(30);

}

void draw(){
background(200);
text("mouseX= "+round(getY(mouseX)), 10,10);
scale(1,-1);
translate(0, -1*height);
stroke(255);
line(0,0, 100,100)
stroke(0);
for(var i=0; i<width;i++){
	line(i,getY(i), i,getY(i));

}


}

</script>
<script>
var  formulaY ="((.1*x)*(.1*x))";
</script>
</head>
<body>
<canvas id="graph"></canvas>
<input id="formula" value="((.1*x)*(.1*x))">
</body>
</html>
